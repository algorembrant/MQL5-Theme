{
    "name": "mql5-support",
    "publisher": "MQL5-Theme-syntax",
    "displayName": "MQL5 Support",
    "description": "MQL5 Syntax Highlighting and Themes",
    "version": "1.4.7",
    "icon": "icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/algorembrant/MQL5-Theme"
    },
    "engines": {
        "vscode": "^1.70.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "mql5",
                "aliases": [
                    "MQL5",
                    "mql5"
                ],
                "extensions": [
                    ".mq5",
                    ".mqh"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "mql5",
                "scopeName": "source.mql5",
                "path": "./syntaxes/mql5.tmLanguage.json"
            }
        ],
        "configurationDefaults": {
            "[mql5]": {
                "editor.tokenColorCustomizations": {
                    "textMateRules": [
                        {
                            "name": "Comments",
                            "scope": [
                                "comment",
                                "comment.line",
                                "comment.block",
                                "punctuation.definition.comment"
                            ],
                            "settings": {
                                "foreground": "#6A9955",
                                "fontStyle": "italic"
                            }
                        },
                        {
                            "name": "Strings",
                            "scope": [
                                "string",
                                "string.quoted.double",
                                "string.quoted.single"
                            ],
                            "settings": {
                                "foreground": "#CE9178"
                            }
                        },
                        {
                            "name": "Numbers",
                            "scope": [
                                "constant.numeric",
                                "constant.numeric.integer",
                                "constant.numeric.float",
                                "constant.numeric.decimal"
                            ],
                            "settings": {
                                "foreground": "#B5CEA8"
                            }
                        },
                        {
                            "name": "Keywords",
                            "scope": [
                                "keyword",
                                "keyword.control"
                            ],
                            "settings": {
                                "foreground": "#C586C0"
                            }
                        },
                        {
                            "name": "Operators",
                            "scope": [
                                "keyword.operator"
                            ],
                            "settings": {
                                "foreground": "#D4D4D4"
                            }
                        },
                        {
                            "name": "Functions",
                            "scope": [
                                "entity.name.function",
                                "meta.function",
                                "support.function"
                            ],
                            "settings": {
                                "foreground": "#DCDCAA"
                            }
                        },
                        {
                            "name": "Classes",
                            "scope": [
                                "entity.name.class",
                                "entity.name.type.class",
                                "support.class"
                            ],
                            "settings": {
                                "foreground": "#4EC9B0"
                            }
                        },
                        {
                            "name": "Types",
                            "scope": [
                                "storage.type",
                                "support.type"
                            ],
                            "settings": {
                                "foreground": "#569CD6"
                            }
                        },
                        {
                            "name": "Variables",
                            "scope": [
                                "variable",
                                "variable.other"
                            ],
                            "settings": {
                                "foreground": "#9CDCFE"
                            }
                        },
                        {
                            "name": "Global Variables",
                            "scope": [
                                "support.function.global-variable.mql5"
                            ],
                            "settings": {
                                "foreground": "#00CED1"
                            }
                        },
                        {
                            "name": "Trade Functions",
                            "scope": [
                                "support.function.trade.mql5"
                            ],
                            "settings": {
                                "foreground": "#E040FB"
                            }
                        },
                        {
                            "name": "Account Info",
                            "scope": [
                                "support.function.account-info.mql5"
                            ],
                            "settings": {
                                "foreground": "#FF4081"
                            }
                        },
                        {
                            "name": "Market Info",
                            "scope": [
                                "support.function.market-info.mql5"
                            ],
                            "settings": {
                                "foreground": "#7C4DFF"
                            }
                        },
                        {
                            "name": "Chart Functions",
                            "scope": [
                                "support.function.chart.mql5"
                            ],
                            "settings": {
                                "foreground": "#536DFE"
                            }
                        },
                        {
                            "name": "Object Functions",
                            "scope": [
                                "support.function.object.mql5"
                            ],
                            "settings": {
                                "foreground": "#448AFF"
                            }
                        },
                        {
                            "name": "Check Functions",
                            "scope": [
                                "support.function.check.mql5"
                            ],
                            "settings": {
                                "foreground": "#40C4FF"
                            }
                        },
                        {
                            "name": "Array Functions",
                            "scope": [
                                "support.function.array.mql5"
                            ],
                            "settings": {
                                "foreground": "#18FFFF"
                            }
                        },
                        {
                            "name": "Conversion Functions",
                            "scope": [
                                "support.function.conversion.mql5"
                            ],
                            "settings": {
                                "foreground": "#64FFDA"
                            }
                        },
                        {
                            "name": "Math Functions",
                            "scope": [
                                "support.function.math.mql5"
                            ],
                            "settings": {
                                "foreground": "#69F0AE"
                            }
                        },
                        {
                            "name": "String Functions",
                            "scope": [
                                "support.function.string.mql5"
                            ],
                            "settings": {
                                "foreground": "#B2FF59"
                            }
                        },
                        {
                            "name": "Date/Time Functions",
                            "scope": [
                                "support.function.datetime.mql5"
                            ],
                            "settings": {
                                "foreground": "#EEFF41"
                            }
                        },
                        {
                            "name": "File Functions",
                            "scope": [
                                "support.function.file.mql5"
                            ],
                            "settings": {
                                "foreground": "#FFFF00"
                            }
                        },
                        {
                            "name": "Custom Indicators",
                            "scope": [
                                "support.function.custom-indicator.mql5"
                            ],
                            "settings": {
                                "foreground": "#FFD740"
                            }
                        },
                        {
                            "name": "Signal Functions",
                            "scope": [
                                "support.function.signal.mql5"
                            ],
                            "settings": {
                                "foreground": "#FFAB40"
                            }
                        },
                        {
                            "name": "Text Functions",
                            "scope": [
                                "support.function.text.mql5"
                            ],
                            "settings": {
                                "foreground": "#FF6E40"
                            }
                        },
                        {
                            "name": "Event Functions",
                            "scope": [
                                "support.function.event.mql5"
                            ],
                            "settings": {
                                "foreground": "#FF5252"
                            }
                        },
