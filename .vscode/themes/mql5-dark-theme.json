{
    "$schema": "vscode://schemas/color-theme",
    "semanticHighlighting": true,
    "name": "MQL5 Dark Professional",
    "type": "dark",
    "colors": {
        "editor.background": "#1e1e1e",
        "editor.foreground": "#d4d4d4",
        "editor.lineHighlightBackground": "#2a2a2a",
        "editor.selectionBackground": "#264f78",
        "editor.selectionHighlightBackground": "#264f7844",
        "editorCursor.foreground": "#aeafad",
        "editorLineNumber.foreground": "#858585",
        "editorLineNumber.activeForeground": "#c6c6c6",
        "editorIndentGuide.background1": "#404040",
        "editorIndentGuide.activeBackground1": "#707070",
        "editor.findMatchBackground": "#515c6a",
        "editor.findMatchHighlightBackground": "#ea5c0055",
        "editorBracketMatch.background": "#0064001a",
        "editorBracketMatch.border": "#888888",
        "editorGroupHeader.tabsBackground": "#252526",
        "tab.activeBackground": "#1e1e1e",
        "tab.inactiveBackground": "#2d2d2d",
        "tab.activeForeground": "#ffffff",
        "tab.inactiveForeground": "#ffffff80",
        "sideBar.background": "#252526",
        "sideBar.foreground": "#cccccc",
        "sideBarTitle.foreground": "#bbbbbb",
        "activityBar.background": "#333333",
        "activityBar.foreground": "#ffffff",
        "statusBar.background": "#007acc",
        "statusBar.foreground": "#ffffff",
        "terminal.background": "#1e1e1e",
        "terminal.foreground": "#cccccc"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "comment.line",
                "comment.block",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#6A9955",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "meta.function entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#DCDCAA"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.class",
                "entity.name.type.class",
                "entity.name.struct",
                "entity.name.type.struct",
                "support.class",
                "support.type"
            ],
            "settings": {
                "foreground": "#4EC9B0"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.quoted.double",
                "string.quoted.single"
            ],
            "settings": {
                "foreground": "#CE9178"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric",
                "constant.numeric.integer",
                "constant.numeric.float",
                "constant.numeric.decimal"
            ],
            "settings": {
                "foreground": "#B5CEA8"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator",
                "keyword.operator.arithmetic",
                "keyword.operator.comparison",
                "keyword.operator.logical",
                "keyword.operator.assignment"
            ],
            "settings": {
                "foreground": "#D4D4D4"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "variable.other"
            ],
            "settings": {
                "foreground": "#9CDCFE"
            }
        },
        {
            "name": "Parameters",
            "scope": [
                "variable.parameter",
                "variable.parameter.function"
            ],
            "settings": {
                "foreground": "#9CDCFE",
                "fontStyle": "italic"
            }
        },
        {
            "name": "IncludePaths",
            "scope": [
                "string.quoted.other.lt-gt",
                "string.quoted.include",
                "entity.name.other.preprocessor.macro.include"
            ],
            "settings": {
                "foreground": "#CE9178"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation.definition.begin.bracket",
                "punctuation.definition.end.bracket",
                "punctuation.section.parens",
                "punctuation.brackets",
                "meta.brace",
                "punctuation.separator",
                "punctuation.terminator",
                "punctuation.accessor"
            ],
            "settings": {
                "foreground": "#D4D4D4"
            }
        },
        {
            "name": "Enums",
            "scope": [
                "entity.name.type.enum",
                "variable.other.enummember"
            ],
            "settings": {
                "foreground": "#4FC1FF"
            }
        },
        {
            "name": "Preprocessor Directives",
            "scope": [
                "keyword.other.preprocessor",
                "meta.preprocessor",
                "keyword.control.directive",
                "punctuation.definition.directive",
                "keyword.other.directive"
            ],
            "settings": {
                "foreground": "#C586C0"
            }
        },
        {
            "name": "Data Types",
            "scope": [
                "storage.type",
                "storage.type.primitive",
                "storage.type.built-in",
                "keyword.type",
                "support.type.primitive"
            ],
            "settings": {
                "foreground": "#4EC9B0"
            }
        },
        {
            "name": "Control Flow Keywords",
            "scope": [
                "keyword.control",
                "keyword.control.conditional",
                "keyword.control.loop",
                "keyword.control.flow"
            ],
            "settings": {
                "foreground": "#C586C0"
            }
        },
        {
            "name": "Storage Modifiers",
            "scope": [
                "storage.modifier",
                "keyword.other.input",
                "keyword.other.extern"
            ],
            "settings": {
                "foreground": "#569CD6"
            }
        },
        {
            "name": "Trade Functions",
            "scope": [
                "support.function.trade.mql5"
            ],
            "settings": {
                "foreground": "#FF6B6B",
                "fontStyle": "bold"
            }
        },
        {
            "name": "CTrade Class Methods",
            "scope": [
                "support.function.ctrade.mql5"
            ],
            "settings": {
                "foreground": "#EE5A5A"
            }
        },
        {
            "name": "Trade Request Actions (ENUM_TRADE_REQUEST_ACTIONS)",
            "scope": [
                "support.constant.trade-action.mql5"
            ],
            "settings": {
                "foreground": "#FF9F43",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Order Type Constants (ENUM_ORDER_TYPE)",
            "scope": [
                "support.constant.order-type.mql5"
            ],
            "settings": {
                "foreground": "#50FA7B"
            }
        },
        {
            "name": "Order State Constants (ENUM_ORDER_STATE)",
            "scope": [
                "support.constant.order-state.mql5"
            ],
            "settings": {
                "foreground": "#5AE05A"
            }
        },
        {
            "name": "Order Filling Modes (ENUM_ORDER_TYPE_FILLING)",
            "scope": [
                "support.constant.order-filling.mql5"
            ],
            "settings": {
                "foreground": "#69F0AE"
            }
        },
        {
            "name": "Order Time Types (ENUM_ORDER_TYPE_TIME)",
            "scope": [
                "support.constant.order-time.mql5"
            ],
            "settings": {
                "foreground": "#A5D6A7"
            }
        },
        {
            "name": "Position Constants (ENUM_POSITION_TYPE)",
            "scope": [
                "support.constant.position-type.mql5"
            ],
            "settings": {
                "foreground": "#64B5F6"
            }
        },
        {
            "name": "Position Properties (ENUM_POSITION_PROPERTY_INTEGER/DOUBLE/STRING)",
            "scope": [
                "support.constant.position-property.mql5"
            ],
            "settings": {
                "foreground": "#42A5F5"
            }
        },
        {
            "name": "Symbol ENUM Constants (ENUM_SYMBOL_INFO_...)",
            "scope": [
                "support.constant.symbol-info.mql5"
            ],
            "settings": {
                "foreground": "#BA68C8"
            }
        },
        {
            "name": "Symbol Trade Modes (ENUM_SYMBOL_TRADE_MODE)",
            "scope": [
                "support.constant.symbol-trade-mode.mql5"
            ],
            "settings": {
                "foreground": "#AB47BC"
            }
        },
        {
            "name": "Symbol Filling Modes (ENUM_SYMBOL_FILLING_MODE)",
            "scope": [
                "support.constant.symbol-filling.mql5"
            ],
            "settings": {
                "foreground": "#9C27B0"
            }
        },
        {
            "name": "Account ENUM Constants (ENUM_ACCOUNT_INFO_...)",
            "scope": [
                "support.constant.account-info.mql5"
            ],
            "settings": {
                "foreground": "#FF79C6"
            }
        },
        {
            "name": "Account Trade Modes (ENUM_ACCOUNT_TRADE_MODE)",
            "scope": [
                "support.constant.account-trade-mode.mql5"
            ],
            "settings": {
                "foreground": "#FF69B4"
            }
        },
        {
            "name": "Account Stop Out Modes (ENUM_ACCOUNT_STOPOUT_MODE)",
            "scope": [
                "support.constant.account-stopout.mql5"
            ],
            "settings": {
                "foreground": "#FF5C93"
            }
        },
        {
            "name": "Deal Constants (ENUM_DEAL_...)",
            "scope": [
                "support.constant.deal.mql5"
            ],
            "settings": {
                "foreground": "#F48FB1"
            }
        },
        {
            "name": "Timeseries Functions",
            "scope": [
                "support.function.timeseries.mql5"
            ],
            "settings": {
                "foreground": "#81D4FA"
            }
        },
        {
            "name": "Timeframe Constants (ENUM_TIMEFRAMES)",
            "scope": [
                "support.constant.timeframe.mql5"
            ],
            "settings": {
                "foreground": "#F1FA8C"
            }
        },
        {
            "name": "Symbol/Market Info Functions",
            "scope": [
                "support.function.symbol-info.mql5"
            ],
            "settings": {
                "foreground": "#CE93D8"
            }
        },
        {
            "name": "Account Info Functions",
            "scope": [
                "support.function.account-info.mql5"
            ],
            "settings": {
                "foreground": "#F8BBD9"
            }
        },
        {
            "name": "Common Functions",
            "scope": [
                "support.function.common.mql5"
            ],
            "settings": {
                "foreground": "#DCDCAA"
            }
        },
        {
            "name": "Conversion Functions",
            "scope": [
                "support.function.conversion.mql5"
            ],
            "settings": {
                "foreground": "#FFD54F"
            }
        },
        {
            "name": "String Functions",
            "scope": [
                "support.function.string.mql5"
            ],
            "settings": {
                "foreground": "#FFC107"
            }
        },
        {
            "name": "Array Functions",
            "scope": [
                "support.function.array.mql5"
            ],
            "settings": {
                "foreground": "#FFAB00"
            }
        },
        {
            "name": "Math Functions",
            "scope": [
                "support.function.math.mql5"
            ],
            "settings": {
                "foreground": "#FFE082"
            }
        },
        {
            "name": "File Functions",
            "scope": [
                "support.function.file.mql5"
            ],
            "settings": {
                "foreground": "#FFCC80"
            }
        },
        {
            "name": "MQL5 Structures",
            "scope": [
                "support.type.struct.mql5"
            ],
            "settings": {
                "foreground": "#FFB86C",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Standard Library Classes",
            "scope": [
                "support.class.stdlib.mql5"
            ],
            "settings": {
                "foreground": "#8BE9FD",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Magic/Predefined Variables",
            "scope": [
                "support.variable.predefined.mql5"
            ],
            "settings": {
                "foreground": "#BD93F9"
            }
        },
        {
            "name": "Event Handler Functions",
            "scope": [
                "support.function.event.mql5"
            ],
            "settings": {
                "foreground": "#FF5555",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Return Codes (ENUM_INIT_RETCODE)",
            "scope": [
                "support.constant.init-retcode.mql5"
            ],
            "settings": {
                "foreground": "#F06292"
            }
        },
        {
            "name": "Terminal/Environment Constants",
            "scope": [
                "support.constant.terminal.mql5"
            ],
            "settings": {
                "foreground": "#90CAF9"
            }
        },
        {
            "name": "Object Property Constants (OBJPROP_...)",
            "scope": [
                "support.constant.objprop.mql5"
            ],
            "settings": {
                "foreground": "#80CBC4"
            }
        },
        {
            "name": "Boolean Constants",
            "scope": [
                "constant.language.boolean",
                "constant.language.true",
                "constant.language.false"
            ],
            "settings": {
                "foreground": "#56B6C2"
            }
        },
        {
            "name": "Numeric Limits",
            "scope": [
                "support.constant.numeric-limits.mql5"
            ],
            "settings": {
                "foreground": "#E5C07B"
            }
        },
        {
            "name": "Color Constants",
            "scope": [
                "support.constant.color.mql5"
            ],
            "settings": {
                "foreground": "#C3E88D"
            }
        },
        {
            "name": "Chart Constants (ENUM_CHART_...)",
            "scope": [
                "support.constant.chart.mql5"
            ],
            "settings": {
                "foreground": "#89DDFF"
            }
        },
        {
            "name": "Object Types (ENUM_OBJECT)",
            "scope": [
                "support.constant.object-type.mql5"
            ],
            "settings": {
                "foreground": "#82AAFF"
            }
        }
    ],
    "semanticTokenColors": {
        "function": "#DCDCAA",
        "function.declaration": "#DCDCAA",
        "variable": "#9CDCFE",
        "variable.declaration": "#9CDCFE",
        "variable.readonly": "#4FC1FF",
        "variable.defaultLibrary": "#BD93F9",
        "parameter": "#9CDCFE",
        "type": "#4EC9B0",
        "class": "#4EC9B0",
        "struct": "#4EC9B0",
        "enum": "#4FC1FF",
        "enumMember": "#4FC1FF",
        "property": "#9CDCFE",
        "method": "#DCDCAA",
        "macro": "#C586C0",
        "comment": "#6A9955"
    }
}