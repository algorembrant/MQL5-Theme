{
    "$schema": "vscode://schemas/color-theme",
    "semanticHighlighting": true,
    "name": "MQL5 High Contrast",
    "type": "hc",
    "colors": {
        "editor.background": "#000000",
        "editor.foreground": "#ffffff",
        "editor.lineHighlightBorder": "#f38518",
        "editor.selectionBackground": "#ffffff",
        "editor.selectionForeground": "#000000",
        "editorCursor.foreground": "#ffffff",
        "editorLineNumber.foreground": "#ffffff",
        "editorLineNumber.activeForeground": "#ffffff",
        "editorIndentGuide.background1": "#ffffff",
        "editorIndentGuide.activeBackground1": "#ffffff"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "comment.line",
                "comment.block",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#7ca668"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "meta.function entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#ffff00"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.class",
                "entity.name.type.class",
                "entity.name.struct",
                "entity.name.type.struct",
                "support.class",
                "support.type"
            ],
            "settings": {
                "foreground": "#00ff00"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.quoted.double",
                "string.quoted.single"
            ],
            "settings": {
                "foreground": "#5ce632"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric",
                "constant.numeric.integer",
                "constant.numeric.float",
                "constant.numeric.decimal"
            ],
            "settings": {
                "foreground": "#b5cea8"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator",
                "keyword.operator.arithmetic",
                "keyword.operator.comparison",
                "keyword.operator.logical",
                "keyword.operator.assignment"
            ],
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "variable.other"
            ],
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Parameters",
            "scope": [
                "variable.parameter",
                "variable.parameter.function"
            ],
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "IncludePaths",
            "scope": [
                "string.quoted.other.lt-gt",
                "string.quoted.include",
                "entity.name.other.preprocessor.macro.include"
            ],
            "settings": {
                "foreground": "#ce9178"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation.definition.begin.bracket",
                "punctuation.definition.end.bracket",
                "punctuation.section.parens",
                "punctuation.brackets",
                "meta.brace",
                "punctuation.separator",
                "punctuation.terminator",
                "punctuation.accessor"
            ],
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Enums",
            "scope": [
                "entity.name.type.enum",
                "variable.other.enummember"
            ],
            "settings": {
                "foreground": "#4fc1ff"
            }
        },
        {
            "name": "Preprocessor Directives",
            "scope": [
                "keyword.other.preprocessor",
                "meta.preprocessor",
                "keyword.control.directive",
                "punctuation.definition.directive",
                "keyword.other.directive"
            ],
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "name": "Data Types",
            "scope": [
                "storage.type",
                "storage.type.primitive",
                "storage.type.built-in",
                "keyword.type",
                "support.type.primitive"
            ],
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "name": "Control Flow Keywords",
            "scope": [
                "keyword.control",
                "keyword.control.conditional",
                "keyword.control.loop",
                "keyword.control.flow"
            ],
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "name": "Storage Modifiers",
            "scope": [
                "storage.modifier",
                "keyword.other.input",
                "keyword.other.extern"
            ],
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "name": "Trade Functions",
            "scope": [
                "support.function.trade.mql5"
            ],
            "settings": {
                "foreground": "#ff0000",
                "fontStyle": "bold"
            }
        },
        {
            "name": "CTrade Class Methods",
            "scope": [
                "support.function.ctrade.mql5"
            ],
            "settings": {
                "foreground": "#ff0000"
            }
        },
        {
            "name": "Trade Request Actions",
            "scope": [
                "support.constant.trade-action.mql5"
            ],
            "settings": {
                "foreground": "#ff0000",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Order Type Constants",
            "scope": [
                "support.constant.order-type.mql5"
            ],
            "settings": {
                "foreground": "#00ff00"
            }
        },
        {
            "name": "Order State Constants",
            "scope": [
                "support.constant.order-state.mql5"
            ],
            "settings": {
                "foreground": "#00ff00"
            }
        },
        {
            "name": "Order Filling Modes",
            "scope": [
                "support.constant.order-filling.mql5"
            ],
            "settings": {
                "foreground": "#00ff00"
            }
        },
        {
            "name": "Order Time Types",
            "scope": [
                "support.constant.order-time.mql5"
            ],
            "settings": {
                "foreground": "#00ff00"
            }
        },
        {
            "name": "Position Constants",
            "scope": [
                "support.constant.position-type.mql5"
            ],
            "settings": {
                "foreground": "#00ff00"
            }
        },
        {
            "name": "Position Properties",
            "scope": [
                "support.constant.position-property.mql5"
            ],
            "settings": {
                "foreground": "#00ff00"
            }
        },
        {
            "name": "Symbol ENUM Constants",
            "scope": [
                "support.constant.symbol-info.mql5"
            ],
            "settings": {
                "foreground": "#00ff00"
            }
        },
        {
            "name": "Symbol Trade Modes",
            "scope": [
                "support.constant.symbol-trade-mode.mql5"
            ],
            "settings": {
                "foreground": "#00ff00"
            }
        },
        {
            "name": "Symbol Filling Modes",
            "scope": [
                "support.constant.symbol-filling.mql5"
            ],
            "settings": {
                "foreground": "#00ff00"
            }
        },
        {
            "name": "Account ENUM Constants",
            "scope": [
                "support.constant.account-info.mql5"
            ],
            "settings": {
                "foreground": "#ff0000"
            }
        },
        {
            "name": "Account Trade Modes",
            "scope": [
                "support.constant.account-trade-mode.mql5"
            ],
            "settings": {
                "foreground": "#ff0000"
            }
        },
        {
            "name": "Account Stop Out Modes",
            "scope": [
                "support.constant.account-stopout.mql5"
            ],
