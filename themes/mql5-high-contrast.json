            }
        },
        {
            "name": "Object Property Constants",
            "scope": [
                "support.constant.objprop.mql5"
            ],
            "settings": {
                "foreground": "#00ff00"
            }
        },
        {
            "name": "Boolean Constants",
            "scope": [
                "constant.language.boolean",
                "constant.language.true",
                "constant.language.false"
            ],
            "settings": {
                "foreground": "#00ff00"
            }
        },
        {
            "name": "Numeric Limits",
            "scope": [
                "support.constant.numeric-limits.mql5"
            ],
            "settings": {
                "foreground": "#b5cea8"
            }
        },
        {
            "name": "Color Constants",
            "scope": [
                "support.constant.color.mql5"
            ],
            "settings": {
                "foreground": "#b5cea8"
            }
        },
        {
            "name": "Chart Constants",
            "scope": [
                "support.constant.chart.mql5"
            ],
            "settings": {
                "foreground": "#00ff00"
            }
        },
        {
            "name": "Object Types",
            "scope": [
                "support.constant.object-type.mql5"
            ],
            "settings": {
                "foreground": "#00ff00"
            }
        }
    ],
    "semanticTokenColors": {
        "function": "#ffff00",
        "variable": "#ffffff",
        "type": "#00ff00",
        "class": "#00ff00",
        "enum": "#4fc1ff",
        "keyword": "#569cd6",
        "string": "#5ce632",
        "number": "#b5cea8",
        "comment": "#7ca668"
    }
}