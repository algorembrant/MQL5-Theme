            "settings": {
                "foreground": "#2aa198"
            }
        },
        {
            "name": "Order Time Types",
            "scope": [
                "support.constant.order-time.mql5"
            ],
            "settings": {
                "foreground": "#2aa198"
            }
        },
        {
            "name": "Position Constants",
            "scope": [
                "support.constant.position-type.mql5"
            ],
            "settings": {
                "foreground": "#268bd2"
            }
        },
        {
            "name": "Position Properties",
            "scope": [
                "support.constant.position-property.mql5"
            ],
            "settings": {
                "foreground": "#268bd2"
            }
        },
        {
            "name": "Symbol ENUM Constants",
            "scope": [
                "support.constant.symbol-info.mql5"
            ],
            "settings": {
                "foreground": "#6c71c4"
            }
        },
        {
            "name": "Symbol Trade Modes",
            "scope": [
                "support.constant.symbol-trade-mode.mql5"
            ],
            "settings": {
                "foreground": "#6c71c4"
            }
        },
        {
            "name": "Symbol Filling Modes",
            "scope": [
                "support.constant.symbol-filling.mql5"
            ],
