            }
        },
        {
            "name": "Boolean Constants",
            "scope": [
                "constant.language.boolean",
                "constant.language.true",
                "constant.language.false"
            ],
            "settings": {
                "foreground": "#d33682"
            }
        },
        {
            "name": "Numeric Limits",
            "scope": [
                "support.constant.numeric-limits.mql5"
            ],
            "settings": {
                "foreground": "#d33682"
            }
        },
        {
            "name": "Color Constants",
            "scope": [
                "support.constant.color.mql5"
            ],
            "settings": {
                "foreground": "#d33682"
            }
        },
        {
            "name": "Chart Constants",
            "scope": [
                "support.constant.chart.mql5"
            ],
            "settings": {
                "foreground": "#268bd2"
            }
        },
        {
            "name": "Object Types",
            "scope": [
                "support.constant.object-type.mql5"
            ],
            "settings": {
                "foreground": "#268bd2"
            }
        }
    ],
    "semanticTokenColors": {
        "function": "#268bd2",
        "variable": "#839496",
        "type": "#b58900",
        "class": "#b58900",
        "enum": "#b58900",
        "keyword": "#859900",
        "string": "#2aa198",
        "number": "#d33682",
        "comment": "#586e75"
    }
}