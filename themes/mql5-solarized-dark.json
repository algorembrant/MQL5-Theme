{
    "$schema": "vscode://schemas/color-theme",
    "semanticHighlighting": true,
    "name": "MQL5 Solarized Dark",
    "type": "dark",
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "comment.line",
                "comment.block",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#586e75",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "meta.function entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#268bd2"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.class",
                "entity.name.type.class",
                "entity.name.struct",
                "entity.name.type.struct",
                "support.class",
                "support.type"
            ],
            "settings": {
                "foreground": "#b58900"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.quoted.double",
                "string.quoted.single"
            ],
            "settings": {
                "foreground": "#2aa198"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric",
                "constant.numeric.integer",
                "constant.numeric.float",
                "constant.numeric.decimal"
            ],
            "settings": {
                "foreground": "#d33682"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator",
                "keyword.operator.arithmetic",
                "keyword.operator.comparison",
                "keyword.operator.logical",
                "keyword.operator.assignment"
            ],
            "settings": {
                "foreground": "#859900"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "variable.other"
            ],
            "settings": {
                "foreground": "#839496"
            }
        },
        {
            "name": "Parameters",
            "scope": [
                "variable.parameter",
                "variable.parameter.function"
            ],
            "settings": {
                "foreground": "#839496",
                "fontStyle": "italic"
            }
        },
        {
            "name": "IncludePaths",
            "scope": [
                "string.quoted.other.lt-gt",
                "string.quoted.include",
                "entity.name.other.preprocessor.macro.include"
            ],
            "settings": {
                "foreground": "#cb4b16"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation.definition.begin.bracket",
                "punctuation.definition.end.bracket",
                "punctuation.section.parens",
                "punctuation.brackets",
                "meta.brace",
                "punctuation.separator",
                "punctuation.terminator",
                "punctuation.accessor"
            ],
            "settings": {
                "foreground": "#839496"
            }
        },
        {
            "name": "Enums",
            "scope": [
                "entity.name.type.enum",
                "variable.other.enummember"
            ],
            "settings": {
                "foreground": "#b58900"
            }
        },
        {
            "name": "Preprocessor Directives",
            "scope": [
                "keyword.other.preprocessor",
                "meta.preprocessor",
                "keyword.control.directive",
                "punctuation.definition.directive",
                "keyword.other.directive"
            ],
            "settings": {
                "foreground": "#cb4b16"
            }
        },
        {
            "name": "Data Types",
            "scope": [
                "storage.type",
                "storage.type.primitive",
                "storage.type.built-in",
                "keyword.type",
                "support.type.primitive"
            ],
            "settings": {
                "foreground": "#b58900"
            }
        },
        {
            "name": "Control Flow Keywords",
            "scope": [
                "keyword.control",
                "keyword.control.conditional",
                "keyword.control.loop",
                "keyword.control.flow"
            ],
            "settings": {
                "foreground": "#859900"
            }
        },
        {
            "name": "Storage Modifiers",
            "scope": [
                "storage.modifier",
                "keyword.other.input",
                "keyword.other.extern"
            ],
            "settings": {
                "foreground": "#859900"
            }
        },
        {
            "name": "Trade Functions",
            "scope": [
                "support.function.trade.mql5"
            ],
            "settings": {
                "foreground": "#dc322f",
                "fontStyle": "bold"
            }
        },
        {
            "name": "CTrade Class Methods",
            "scope": [
                "support.function.ctrade.mql5"
            ],
            "settings": {
                "foreground": "#cb4b16"
            }
        },
        {
            "name": "Trade Request Actions",
            "scope": [
                "support.constant.trade-action.mql5"
            ],
            "settings": {
                "foreground": "#d33682",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Order Type Constants",
            "scope": [
                "support.constant.order-type.mql5"
            ],
            "settings": {
                "foreground": "#2aa198"
            }
        },
        {
            "name": "Order State Constants",
            "scope": [
                "support.constant.order-state.mql5"
            ],
            "settings": {
                "foreground": "#2aa198"
            }
        },
        {
