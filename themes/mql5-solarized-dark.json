            }
        },
        {
            "name": "Account ENUM Constants",
            "scope": [
                "support.constant.account-info.mql5"
            ],
            "settings": {
                "foreground": "#d33682"
            }
        },
        {
            "name": "Account Trade Modes",
            "scope": [
                "support.constant.account-trade-mode.mql5"
            ],
            "settings": {
                "foreground": "#d33682"
            }
        },
        {
