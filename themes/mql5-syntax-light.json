{
    "$schema": "vscode://schemas/color-theme",
    "semanticHighlighting": true,
    "name": "MQL5 Syntax (Light)",
    "type": "light",
    "colors": {},
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "comment.line",
                "comment.block",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#5C6370",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "meta.function entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#795548"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.class",
                "entity.name.type.class",
                "entity.name.struct",
                "entity.name.type.struct",
                "support.class",
                "support.type"
            ],
            "settings": {
                "foreground": "#0184BC"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.quoted.double",
                "string.quoted.single"
            ],
            "settings": {
                "foreground": "#50A14F"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric",
                "constant.numeric.integer",
                "constant.numeric.float",
                "constant.numeric.decimal"
            ],
            "settings": {
                "foreground": "#986801"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator",
                "keyword.operator.arithmetic",
                "keyword.operator.comparison",
                "keyword.operator.logical",
                "keyword.operator.assignment"
            ],
            "settings": {
                "foreground": "#383A42"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "variable.other"
            ],
            "settings": {
                "foreground": "#E45649"
            }
        },
        {
            "name": "Parameters",
            "scope": [
                "variable.parameter",
                "variable.parameter.function",
                "variable.other.input.mql5"
            ],
            "settings": {
                "foreground": "#E45649",
                "fontStyle": "italic"
            }
        },
        {
            "name": "IncludePaths",
            "scope": [
                "string.quoted.other.lt-gt",
                "string.quoted.include",
                "entity.name.other.preprocessor.macro.include"
            ],
            "settings": {
                "foreground": "#50A14F"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation.definition.begin.bracket",
                "punctuation.definition.end.bracket",
                "punctuation.section.parens",
                "punctuation.brackets",
                "meta.brace",
                "punctuation.separator",
                "punctuation.terminator",
                "punctuation.accessor"
            ],
            "settings": {
                "foreground": "#383A42"
            }
        },
        {
            "name": "Enums",
            "scope": [
                "entity.name.type.enum",
                "variable.other.enummember",
                "constant.other.caps.mql5"
            ],
            "settings": {
                "foreground": "#4078F2"
            }
        },
        {
            "name": "Preprocessor Directives",
            "scope": [
                "keyword.other.preprocessor",
                "meta.preprocessor",
                "keyword.control.directive",
                "punctuation.definition.directive",
                "keyword.other.directive"
            ],
            "settings": {
                "foreground": "#A626A4"
            }
        },
        {
            "name": "Data Types",
            "scope": [
                "storage.type",
                "storage.type.primitive",
                "storage.type.built-in",
                "keyword.type",
                "support.type.primitive"
            ],
            "settings": {
                "foreground": "#0184BC"
            }
        },
        {
            "name": "Control Flow Keywords",
            "scope": [
                "keyword.control",
                "keyword.control.conditional",
                "keyword.control.loop",
                "keyword.control.flow"
            ],
            "settings": {
                "foreground": "#D02090"
            }
        },
        {
            "name": "Storage Modifiers",
            "scope": [
                "storage.modifier",
                "keyword.other.input",
                "keyword.other.extern"
            ],
            "settings": {
                "foreground": "#4078F2"
            }
        },
        {
            "name": "Trade Functions",
            "scope": [
                "support.function.trade.mql5"
            ],
            "settings": {
                "foreground": "#C91D1D",
                "fontStyle": "bold"
            }
        },
        {
            "name": "CTrade Class Methods",
